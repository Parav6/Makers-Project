{
  "manifest_version": 3,
  "name": "Screen Notes",
  "version": "1.0",
  "description": "A Chrome extension to take screenshots of the current webpage.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["tabs", "activeTab"],
  "host_permissions": ["<all-links>"],
  "action": {
    "default_popup": "popup.html"
  }
}


  
  